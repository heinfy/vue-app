{
  "postcss-import": {},
  "postcss-url": {},
  "postcss-aspect-ratio-mini": {},
  "postcss-write-svg": {
    "utf8": false
  },
  "postcss-cssnext": {},
  "postcss-viewport-units": {},
  "cssnano": {
    "preset": "advanced",
    "autoprefixer": false,
    "postcss-zindex": false
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-px-to-viewport": {
        "viewportWidth": 375,
        "viewportHeight": 667,
        "unitPrecision": 3,
        "viewportUnit": "vw",
        "mixPixelValue": 1,
        "selectorBlackList": [
          ".iphoneXr"
        ]
      }
    }
  },
  "plugins": {
    "autoprefixer": {},
    "postcss-px-to-viewport": {
      "viewportWidth": 375,
      "viewportHeight": 667,
      "unitPrecision": 3,
      "viewportUnit": "vw",
      "mixPixelValue": 1,
      "selectorBlackList": [
        ".iphoneXr"
      ]
    }
  }
}